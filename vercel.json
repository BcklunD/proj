{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.5.3"
        }
    },
    "routes": [
        { "src": "/favicon.png",  "dest": "/favicon.png" },
        { "src": "/(css|script|include)/(.*)",  "dest": "/$1/$2" },
        { "src": "/(.*)",  "dest": "/api/index.php" }
    ]
}